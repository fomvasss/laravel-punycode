{
    "name": "fomvasss/laravel-punycode",
    "description": "Package for encoding of Unicode for Internationalized Domain Names in Applications (IDNA)",
    "keywords": ["laravel", "punycode", "idn", "domain"],
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "role": "Developer",
            "name": "Fomin Vasyl",
            "email": "fomvasss@gmail.com"
        }
    ],
    "require": {
